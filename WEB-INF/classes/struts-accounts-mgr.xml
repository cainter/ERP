    	  <result name="toDisabledAdjustBill">/WEB-INF/view/accounts/AdjustBillListForCancel.jsp</result>
    	  <result name="disabledAdjustBill">/WEB-INF/view/accounts/AdjustBillListForCancel.jsp</result>
    	  <result name="disabledAllAdjustBill">/WEB-INF/view/accounts/AdjustBillListForCancel.jsp</result>
