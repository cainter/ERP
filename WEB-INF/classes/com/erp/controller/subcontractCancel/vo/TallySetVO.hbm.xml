<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.erp.controller.subcontractCancel.vo.TallySetVO" table="tally_set">
		<id name="id" type="long" column="id">
			<generator class="assigned" />
		</id>
		<property name="title" type="string">
			<column name="title" length="50" not-null="true" />
		</property>
		<property name="enabled" type="string">
			<column name="enabled" length="1" not-null="true" />
		</property>
		<property name="sys" type="string">
			<column name="sys" length="1" not-null="true" />
		</property>
    </class>
</hibernate-mapping>