<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.erp.model.vo.Unit" table="unit">
		<id name="id" type="long" column="id">
			<generator class="assigned" />
		</id>
		<property name="unitName" type="string">
			<column name="unit_name" length="30" not-null="true" />
		</property>
		<property name="sequenceCode" type="int">
			<column name="sequence_code" length="4" not-null="true" />
		</property>
    </class>
</hibernate-mapping>